@model MedOffice.Models.ApplicationUser

@{
    ViewBag.Title = "Usuń dane pacjenta";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="col-md-offset-0-5 col-md-4regt">
    <h2>@ViewBag.Title.</h2>
</div>

<div>
    <br style="clear: both;">
    <div class="col-md-offset-0-5 col-md-4regst">
        <h4>Potwierdź usunięcie pracownika.</h4>
    </div>
    <br style="clear: both;">
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayName("Imię:")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Name)
        </dd>

        <dt>
            @Html.DisplayName("Nazwisko:")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Surname)
        </dd>

        <dt>
            @Html.DisplayName("Pesel:")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.UserName)
        </dd>

        <dt>
            @Html.DisplayName("Email:")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Email)
        </dd>
    </dl>

    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Usuń" class="btn btn-default" /> |
            @Html.ActionLink("Powrót do listy", "WorkerSearch", "Search")
        </div>
    }
</div>
