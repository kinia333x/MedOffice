@model IEnumerable<MedOffice.Models.AppointmentViewModels.ServiceViewModel>

@{
    Layout = "~/Views/Shared/_PdfLayout.cshtml";
    ViewBag.Title = "Report";
}

 <h2>Raport</h2>

 <table class="table">
     <tr>
         <th>
             @Html.DisplayNameFor(model => model.ServiceType)
         </th>
         <th>
             @Html.DisplayNameFor(model => model.ServiceName)
         </th>
         <th>
             @Html.DisplayNameFor(model => model.ServiceDate)
         </th>
         <th>
             @Html.DisplayNameFor(model => model.ServicePrice)
         </th>
         <th>
             @Html.DisplayNameFor(model => model.SuppliesPrice)
         </th>
         <th>
             @Html.DisplayNameFor(model => model.TotalPrice)
         </th>
     </tr>
     @foreach (var item in Model)
     {
     <tr>
         <td>
             @Html.DisplayFor(modelItem => item.ServiceType)
         </td>
         <td>
             @Html.DisplayFor(modelItem => item.ServiceName)
         </td>
         <th>
             @Html.DisplayFor(modelItem => item.ServiceDate)
         </th>
         <td>
             @Html.DisplayFor(modelItem => item.ServicePrice)
         </td>
         <td>
             @Html.DisplayFor(modelItem => item.SuppliesPrice)
         </td>
         <th>
             @Html.DisplayFor(modelItem => item.TotalPrice)
         </th>
     </tr>
     }
 </table>